#taking nodes through user in linked list
class Node:


    def __init__(self, data):
        self.data = data
        self.next = None


class LinkedList:


    def __init__(self):
        self.head = None


    def printList(self):
        temp = self.head
        while (temp):
            print(temp.data)
            temp = temp.next


llist = LinkedList()
l1=input()
l2=input()
l3=input()

llist.head = Node(l1)
second = Node(l2)
third = Node(l3)

llist.head.next = second    #first was assigned as second
second.next = third         #second was assigned as third

llist.printList()


